{
  "devDependencies": {
    "@tailwindcss/forms": "^0.3.3",
    "@tailwindcss/typography": "^0.4.1",
    "autoprefixer": "^10.2.6",
    "postcss": "^8.3.5",
    "tailwindcss": "^2.2.2"
  },
  "scripts": {
    "dev": "tailwindcss -i assets/app.css -o public/static/app.css --jit",
    "prod": "tailwindcss -i assets/app.css -o public/static/app.css --jit --purge 'templates/**/*.twig' -m",
    "watch": "tailwindcss -i assets/app.css -o public/static/app.css --jit --purge 'templates/**/*.twig' --watch"
  },
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.2.1"
  }
}
